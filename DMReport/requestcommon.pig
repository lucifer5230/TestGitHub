Path = load 'hdfs://v2namenode:8020/spo/request' using PigStorage('\t') as (
farmId:chararray,userLogin:chararray,siteSubscriptionId:chararray,timestampUtc:chararray,correlationId:chararray,webApplicationId:chararray,serverUrl:chararray,siteId:chararray,siteUrl:chararray,webId:chararray,webUrl:chararray,documentPath:chararray,contentTypeId:chararray,queryString:chararray,bytesConsumed:chararray,httpStatus:chararray,sessionId:chararray,referrerUrl:chararray,referrerQueryString:chararray,browser:chararray,userAgent:chararray,userAddress:chararray,requestCount:chararray,queryCount:chararray,queryDurationSum:chararray,serviceCallCount:chararray,serviceCallDurationSum:chararray,operationCount:chararray,duration:chararray,requestType:chararray,title:chararray,sqlLogicalReads:chararray,cpuMCycles:chararray,cpuDuration:chararray,distributedCacheReads:chararray,distributedCacheReadsDuration:chararray,distributedCacheReadsSize:chararray,distributedCacheWrites:chararray,distributedCacheWritesDuration:chararray,distributedCacheWritesSize:chararray,distributedCacheMisses:chararray,distributedCacheHits:chararray,distributedCacheFailures:chararray,distributedCachedObjectsRequested:chararray,managedMemoryBytes:chararray,managedMemoryBytesLOH:chararray,iisLatency:chararray,requestManagementRoutedServerUrl:chararray,requestManagementThrottled:chararray,requestManagementUploadDuration:chararray,requestManagementResponseDuration:chararray,requestManagementDownloadDuration:chararray,headersForwarded:chararray,claimsAuthenticationTime:chararray,claimsAuthenticationTimeType:chararray,mUIEnabled:chararray,webCulture:chararray,uiCulture:chararray);

Path1 = foreach Path generate timestampUtc, userLogin, documentPath, referrerUrl, requestType, duration, serverUrl, browser, httpStatus;

store Path1 into 'hdfs://v2namenode:8020/spo/request/requestcommon' using PigStorage('\t');
